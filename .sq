sq: 0,
git: 'macos',
require: [
  'git'
],
bin: {
  zsh: {
    info: 'copy zsh config files',
    run: 'cp -ir zsh/ $HOME'
  },
  git: {
    info: 'setup git config',
    run: [
      'git config --global user.name "%GIT_FIRST_NAME% %GIT_LAST_NAME%"',
      'git config --global user.email "%GIT_EMAIL%"',
      'git config --global core.editor "nano"'
    ]
  },
  dns: {
    info: 'setup localhost name',
    run: 'sudo scutil --set LocalHostName %LOCALHOST%'
  }
}
